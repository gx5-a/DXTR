    <script>
        function fakeIPLogger() {
            let discordUser = document.getElementById("discordUserIP").value;
            if (discordUser === "") {
                alert("Please enter a Discord username.");
                return;
            }
            document.getElementById("ipResult").innerHTML = "Discord User: " + discordUser + "<br>IP: " + (Math.floor(Math.random() * 255) + 1) + "." + (Math.floor(Math.random() * 255)) + "." + (Math.floor(Math.random() * 255)) + "." + (Math.floor(Math.random() * 255)) + "<br>Location: Unknown";
        }
        
        function fakeGiftCard() {
            let cardType = document.getElementById("giftCardType").value;
            let code = "";
            let characters = "ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789";
            for (let i = 0; i < 16; i++) {
                code += characters.charAt(Math.floor(Math.random() * characters.length));
                if ((i + 1) % 4 === 0 && i !== 15) code += "-";
            }
            document.getElementById("giftCardResult").innerHTML = "Generated " + cardType + " Code: " + code;
        }
        
        function fakePasswordCrack() {
            let discordUser = document.getElementById("discordUserPass").value;
            if (discordUser === "") {
                alert("Please enter a Discord username.");
                return;
            }
            document.getElementById("passwordResult").innerHTML = "Cracking password for " + discordUser + "...";
            setTimeout(() => {
                document.getElementById("passwordResult").innerHTML = "No password found. Try again later.";
            }, 3000);
        }
    </script>
